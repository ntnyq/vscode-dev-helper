{
  "eslint.enable": true,
  "prettier.enable": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "toml",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "Cves",
    "GHSA",
    "Ghsas",
    "NPMRC",
    "ofetch",
    "taze",
    "tazerc",
    "tsup"
  ],
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "package.json",
        "cmd": "pnpm run generate:meta",
        "isAsync": true
      }
    ]
  }
}
