{
  "publisher": "ntnyq",
  "name": "vscode-dev-helper",
  "displayName": "VSCode Dev Helper",
  "preview": true,
  "version": "0.0.5",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "description": "Personal dev helper built on top of VSCode",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-dev-helper",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/vscode-dev-helper.git"
  },
  "main": "./dist/index.js",
  "icon": "./res/icon.png",
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup",
    "generate:type": "vscode-ext-gen --output=src/meta.ts",
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "dependencies": {
    "ofetch": "^1.4.1",
    "reactive-vscode": "^0.2.6-beta.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.1.0",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.7.7",
    "@types/vscode": "^1.94.0",
    "@vscode/vsce": "^3.2.0",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "vscode-ext-gen": "^0.4.4"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "package.json",
        "url": "./schemas/pnpm.schema.json"
      }
    ],
    "commands": [
      {
        "command": "vscode-dev-helper.enable-codelens",
        "category": "VSCode Dev Helper",
        "title": "Enable CodeLens"
      },
      {
        "command": "vscode-dev-helper.disable-codelens",
        "category": "VSCode Dev Helper",
        "title": "Disable CodeLens"
      },
      {
        "command": "vscode-dev-helper.codelens-action",
        "category": "VSCode Dev Helper",
        "title": "Trigger CodeLens Action"
      }
    ],
    "configuration": {
      "title": "VSCode Dev Helper",
      "properties": {
        "vscode-dev-helper.enableCodeLens": {
          "type": "boolean",
          "default": true,
          "description": "Enable CodeLens"
        }
      }
    }
  },
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-dev-helper/issues/new"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.94.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
