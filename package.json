{
  "publisher": "ntnyq",
  "name": "vscode-dev-helper",
  "displayName": "VSCode Dev Helper",
  "version": "0.0.3",
  "private": true,
  "packageManager": "pnpm@9.0.4",
  "preview": true,
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-dev-helper",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/vscode-dev-helper.git"
  },
  "main": "./dist/index.js",
  "icon": "./res/icon.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup",
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "dependencies": {
    "ofetch": "^1.3.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "2.0.0-beta.26",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^20.12.7",
    "@types/vscode": "^1.88.0",
    "@vscode/vsce": "^2.26.0",
    "bumpp": "^9.4.0",
    "eslint": "^9.1.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "package.json",
        "url": "./schemas/pnpm.schema.json"
      }
    ],
    "commands": [],
    "configuration": {},
    "menus": {
      "editor/context": [],
      "explorer/context": []
    }
  },
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-dev-helper/issues/new"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.83.1"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
