{
  "publisher": "ntnyq",
  "name": "vscode-dev-helper",
  "displayName": "VSCode Dev Helper",
  "version": "0.0.4",
  "private": true,
  "packageManager": "pnpm@9.8.0",
  "preview": true,
  "description": "Personal dev helper built on top of VSCode",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/vscode-dev-helper",
  "repository": {
    "type": "github",
    "url": "https://github.com/ntnyq/vscode-dev-helper.git"
  },
  "main": "./dist/index.js",
  "icon": "./res/icon.png",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "NODE_ENV=development tsup",
    "generate:type": "vscode-ext-gen --output=src/meta.ts",
    "lint": "eslint .",
    "pack": "vsce package --no-dependencies",
    "prepare": "husky",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && pnpm run publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "vscode:prepublish": "pnpm run build"
  },
  "dependencies": {
    "ofetch": "^1.3.4",
    "reactive-vscode": "^0.2.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^2.8.2",
    "@ntnyq/prettier-config": "^1.21.2",
    "@types/node": "^22.5.0",
    "@types/vscode": "^1.92.0",
    "@vscode/vsce": "^3.0.0",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "tsx": "^4.18.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vscode-ext-gen": "^0.4.2"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "jsonValidation": [
      {
        "fileMatch": "package.json",
        "url": "./schemas/pnpm.schema.json"
      }
    ],
    "commands": [],
    "configuration": {},
    "menus": {
      "editor/context": [],
      "explorer/context": []
    }
  },
  "bugs": {
    "url": "https://github.com/ntnyq/vscode-dev-helper/issues/new"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.92.2"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,md,json,yml,yaml}": "eslint --fix"
  }
}
