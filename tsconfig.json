{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESnext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "types": ["node"],
    "resolveJsonModule": true,
    "sourceMap": false,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true
  }
}
